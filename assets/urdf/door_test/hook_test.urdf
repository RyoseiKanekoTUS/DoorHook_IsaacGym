<?xml version="1.0" ?>
<robot name="hook_test">
    <link name="ee_base"/>
    <joint name="ee_x" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="ee_base"/>
        <child link="hand_base"/>
        <axis xyz="1 0 0"/>
        <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.1"/>
    </joint>
    <joint name="ee_y" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="ee_base"/>
        <child link="hand_base"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.1"/>
    </joint>
    <joint name="ee_z" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="ee_base"/>
        <child link="hand_base"/>
        <axis xyz="0 0 1"/>
        <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.1"/>
    </joint>
    <joint name="ee_rx" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="ee_base"/>
        <child link="hand_base"/>
        <axis xyz="1 0 0"/>
        <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.1"/>
    </joint>
    <joint name="ee_ry" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="ee_base"/>
        <child link="hand_base"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.1"/>
    </joint>
    <joint name="ee_rz" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="ee_base"/>
        <child link="hand_base"/>
        <axis xyz="0 0 1"/>
        <limit effort="1000.0" lower="-0.5" upper="0.5" velocity="0.1"/>
    </joint>

    <link name="hand_base">
        <visual>
            <geometry>
                <cylinder length="0.075" radius="0.0365"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.075" radius="0.0365"/>
            </geometry>
        </collision>
    </link>

    <joint name="hand2hook" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="hand_base"/>
        <child link="hook"/>
    </joint>

    <link name="hook">
        <visual>
            <geometry>
                <mesh filename="./meshes/ur3/collision/hook4door.obj"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="./meshes/ur3/collision/hook4door.obj"/>
            </geometry>
        </collision>
    </link>
</robot>
